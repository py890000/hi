<h1>Listing demos</h1>

<table>
  <tr>
    <th>Userinfo</th>
    <th>Login name</th>
    <th>Login password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @demos.each do |demo| %>
  <tr>
    <td><%= demo.userInfo %></td>
    <td><%= demo.login_name %></td>
    <td><%= demo.login_password %></td>
    <td><%= link_to 'Show', demo %></td>
    <td><%= link_to 'Edit', edit_demo_path(demo) %></td>
    <td><%= link_to 'Destroy', demo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

