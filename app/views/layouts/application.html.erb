<!DOCTYPE html>
<html>
	<head>
		<title>Hi</title>
		<meta name="description" content="hi">
		<meta name="author" content="hi py">
		<%= stylesheet_link_tag "/css/custom-theme/jquery-ui-1.8.16.custom.css" %>
		<%= stylesheet_link_tag "/bootstrap/bootstrap.css" %>
		<%= javascript_include_tag "/js/jquery-1.6.2.min.js" %>
		<%= javascript_include_tag  "/js/jquery-ui-1.8.16.custom.min.js"%>

		<!--daterangepicker-->
		<%= javascript_include_tag  "/third-party/jQuery-UI-Date-Range-Picker/js/date.js" %>
		<%= javascript_include_tag  "/third-party/jQuery-UI-Date-Range-Picker/js/daterangepicker.jQuery.js" %>

		<!--wijmo-->
		<%= javascript_include_tag  "/third-party/wijmo/jquery.mousewheel.min.js" %>
		<%= javascript_include_tag  "/third-party/wijmo/jquery.bgiframe-2.1.3-pre.js" %>
		<%= javascript_include_tag  "/third-party/wijmo/jquery.wijmo-open.1.5.0.min.js" %>

		<!-- FileInput -->
		<%= javascript_include_tag "/third-party/jQuery-UI-FileInput/js/enhance.min.js" %>
		<%= javascript_include_tag "/third-party/jQuery-UI-FileInput/js/fileinput.jquery.js" %>

		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>

	</head>
	<body>
		<div class="topbar">
      <div class="fill">
        <div class="container">
					<a class="brand" href="">HI</a>
					<ul class="nav">
						<li class="active">
							<a href="#">主页</a>
						</li>
						<li>
							<a href="">反馈</a>
              </li>
              <li>
							<a href="/articles">博文</a>
              </li>
            <%= render 'devise/menu/registration_items' %>
            <%= render 'devise/menu/login_items' %>      
            

              
					</ul>
					<form action="" class="pull-right">
						<input type="text" placeholder="Search">
					</form>
				</div>
			</div>
    </div>
    <p class="notice"><%= notice %></p>
    <p class="alert"> <%= alert %></p>

		<div class="container">
			<div class="content">
				<div class="page-header">
					<h1>HI Jimmy Boke <small>简单的BOKE</small></h1>
				</div>
				<div class="row">
					<div class="span10">
						<%= yield %>
					</div>
				</div>
			</div>
			<footer>
				<p>
					HI boke &copy; Jimmy  2012.  &copy; Jimmy 2012
				</p>
			</footer>

		</div>

	</body>
</html>
